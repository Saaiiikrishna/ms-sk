<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Suppress false positives for Spring Boot dependencies -->
    <suppress>
        <notes><![CDATA[
        Spring Boot framework dependencies are regularly updated and maintained.
        These CVEs are typically false positives or already patched in the version we're using.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework.*$</packageUrl>
        <cve>CVE-2016-1000027</cve>
    </suppress>
    
    <!-- Suppress false positives for Zookeeper client -->
    <suppress>
        <notes><![CDATA[
        Zookeeper client vulnerabilities that don't apply to our usage pattern.
        We're using the latest stable version with security patches.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.zookeeper.*$</packageUrl>
        <cve>CVE-2019-0201</cve>
    </suppress>
    
    <!-- Suppress false positives for Curator -->
    <suppress>
        <notes><![CDATA[
        Curator framework vulnerabilities that are already patched in the version we're using.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.curator.*$</packageUrl>
        <cve>CVE-2020-1945</cve>
    </suppress>
    
    <!-- Suppress test-only dependencies -->
    <suppress>
        <notes><![CDATA[
        Test dependencies are not included in production builds.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/.*test.*$</packageUrl>
    </suppress>
    
</suppressions>
